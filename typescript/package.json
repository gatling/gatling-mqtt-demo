{
  "name": "gatling-ts-demo",
  "version": "3.14.306",
  "private": true,
  "type": "module",
  "main": "target/bundle",
  "dependencies": {
    "@gatling.io/core": "3.14.500",
    "@gatling.io/mqtt": "3.14.500",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@gatling.io/cli": "3.14.500",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "typescript": "5.9.3"
  },
  "scripts": {
    "clean": "rimraf target",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "check": "tsc --noEmit",
    "build": "tsc --noEmit && gatling build --typescript",
    "recorder": "gatling recorder --typescript"
  }
}
